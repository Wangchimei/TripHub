<%= link_to "CSV", spots_path(format: :csv) %>

<% @spots.each.with_index(1) do |spot, index| %>
  <p><%= index %> <%= spot.name %>
  <%= link_to 'Save', user_spots_path(id: spot.id), method: :post %>

  <p><%= spot.address %></p>
  <p>Admission Fee: <%= spot.admission_fee %></p>
  <p>Time spent: <%= spot.duration %></p>
  <p><%= spot.latitude %>, <%= spot.longitude %></p>
  <br>
<% end %>
